{
  "name": "uikit-webcomponents",
  "version": "0.1.19",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/uikit-webcomponents/uikit-webcomponents.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build:watch": "stencil build --docs --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e --coverage --detectOpenHandles",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "dayjs": "^1.11.3",
    "prettier": "^2.2.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/html": "^6.2.9",
    "@types/jest": "^26.0.23",
    "@types/puppeteer": "^5.4.3",
    "babel-loader": "^8.2.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jsx-dom": "^7.0.0-beta.5",
    "puppeteer": "^5.5.0"
  }
}
